    "file-loader": "^0.11.2",
    "font-awesome-webpack": "0.0.5-beta.2",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lodash": "^4.17.4",
    "lodash.merge": "^4.6.0",
    "manifest-revision-webpack-plugin": "^0.4.0",
    "raw-loader": "^0.5.1",
    "redux-devtools": "^3.4.1",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.18.2",
